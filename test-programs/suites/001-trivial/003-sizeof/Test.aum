pragma Unsafe_Module;

module body Test is
    function Put_Character(character: Nat8): Int32 is
        pragma Foreign_Import(External_Name => "putchar");
    end;

    function main(): ExitCode is
        let size: ByteSize := sizeof(Nat8);
        if size /= 1 then
            Put_Character('f');
        end if;
        return ExitSuccess();
    end;
end module body.

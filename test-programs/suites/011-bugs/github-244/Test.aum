module body Test is
    typeclass MyPrintable(A: Free) is
        method myPrint(value: A): Unit;

        method myPrintSpan(array: Span[A, Static]): Unit;
    end;

    generic [A: Free(MyPrintable)]
    instance MyPrintable(Span[A, Static]) is
        method myPrint(value: Span[A, Static]): Unit is
            return myPrintSpan(value);
        end;

        method myPrintSpan(array: Span[Span[A, Static], Static]): Unit is
            return nil;
        end;
    end;

    function main(): ExitCode is
        return ExitSuccess();
    end;
end module body.
